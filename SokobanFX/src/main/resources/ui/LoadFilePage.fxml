        <Image url="@../StartPage.jpg"/>
    </ImageView>
    <Label alignment="CENTER" layoutX="52.0" layoutY="175.0" prefHeight="25.0" prefWidth="295.0"
           style="-fx-background-color: #8fbc84;" text="Load History Progress" textAlignment="CENTER" textFill="WHITE">
        <font>
            <Font name="Arial Bold" size="14.0"/>
        </font>
        <effect>
            <DropShadow blurType="GAUSSIAN" color="#0f642f" height="10.5" radius="6.077500000000001" width="15.81"/>
        </effect>
    </Label>
    <Label layoutX="108.0" layoutY="217.0" text="* Choose a progress to continue.">
        <font>
            <Font name="Arial Bold" size="12.0"/>
        </font>
    </Label>
